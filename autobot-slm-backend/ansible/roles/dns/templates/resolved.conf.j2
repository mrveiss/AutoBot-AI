# AutoBot systemd-resolved Configuration
# Generated by Ansible - DO NOT EDIT MANUALLY

[Resolve]
{% if systemd_resolved_dns_servers | length > 0 %}
DNS={{ systemd_resolved_dns_servers | join(' ') }}
{% endif %}
{% if systemd_resolved_fallback_dns | length > 0 %}
FallbackDNS={{ systemd_resolved_fallback_dns | join(' ') }}
{% endif %}
Domains={{ dns_search_domains | join(' ') }}
DNSSEC=allow-downgrade
DNSOverTLS=no
MulticastDNS=yes
LLMNR=yes
Cache=yes
CacheFromLocalhost=no
DNSStubListener=yes
ReadEtcHosts=yes
