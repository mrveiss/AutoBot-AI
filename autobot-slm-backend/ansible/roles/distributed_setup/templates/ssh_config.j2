# AutoBot Fleet SSH Configuration
# Generated by Ansible - DO NOT EDIT MANUALLY

Host autobot-fleet-*
    User {{ service_user }}
    IdentityFile {{ ssh_key_path }}
    StrictHostKeyChecking accept-new
    UserKnownHostsFile {{ ansible_env.HOME }}/.ssh/known_hosts_fleet
    ServerAliveInterval 60
    ServerAliveCountMax 3
    ConnectTimeout 10

{% for node in fleet_nodes %}
Host {{ node.name }} autobot-fleet-{{ node.name }}
    HostName {{ node.host }}
    User {{ service_user }}
    IdentityFile {{ ssh_key_path }}

{% endfor %}
